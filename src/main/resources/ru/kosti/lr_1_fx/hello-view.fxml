<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="210.0" prefWidth="548.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.kosti.lr_1_fx.presenter.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <HBox prefHeight="223.0" prefWidth="548.0">
         <children>
            <VBox id="createOrderBox" prefHeight="223.0" prefWidth="152.0">
               <children>
                  <ComboBox id="selectFoodCobobox" prefWidth="150.0" promptText="Select food" />
