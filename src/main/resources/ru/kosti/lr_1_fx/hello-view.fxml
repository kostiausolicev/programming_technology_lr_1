               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </VBox>
            <VBox id="showOrderBox" prefHeight="223.0" prefWidth="204.0" />
            <VBox id="showOrderDetailsBox" prefHeight="223.0" prefWidth="151.0">
               <children>
                  <Label id="foodName" text="Food name:" />
                  <Label text="Composition: " />
                  <Label text="Price" />
               </children></VBox>
         </children>
      </HBox>
